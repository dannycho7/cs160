classA {
     integer x;
     integer y;

     classA(integer xarg, integer yarg) -> none {
         x = xarg;
	 y = yarg;
     }

     inc() -> none {
         x = x * 2;
	 y = y + 1;
     }
}

Main {

     main() -> none {
     	    classA a;
	    integer i;

	    a = new classA(3, 4);
	    print a.x;
	    print a.y;

	    a.inc();

	    print a.x * a.y;
     }

}
